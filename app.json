{
  "expo": {
    "name": "cooksir-app",
    "slug": "cooksir-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/cook_logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "cooksir-app",
    "splash": {
      "image": "./assets/images/cook_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/cook_logo2.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "51896d8f-202c-416f-ad02-a391a410018b"
      }
    }
  }
}
