{
  "expo": {
    "name": "cooksir-app",
    "slug": "cooksir-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/cook_logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "cooksir-app",
    "splash": {
      "image": "./assets/images/cook_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mongkol251.cooksirapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/cook_logo2.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mongkol251.cooksirapp"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "635cd217-b883-4c55-835e-ed69499c3b97"
      }
    }
  }
}
